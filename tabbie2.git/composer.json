{
  "name": "Tabbie2 Webapp",
  "description": "A BP Debate-Tab Webapp written in Yii2",
  "keywords": [
    "BP",
    "tabbing",
    "Debating"
  ],
  "homepage": "http://www.tabbie.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "kartik-v/yii2-datecontrol": "dev-master",
    "kartik-v/yii2-widgets": "dev-master",
    "kartik-v/yii2-grid": "*",
    "kartik-v/yii2-mpdf": "*",
    "kartik-v/yii2-sortable": "*",
    "kartik-v/yii2-widget-typeahead": "*",
    "kartik-v/yii2-popover-x": "*",
    "kartik-v/yii2-checkbox-x": "*",
    "kartik-v/yii2-helpers": "*",
    "rmrevin/yii2-minify-view": "*",
    "jakobreiter/yii2-quagga-js": "dev-master",
    "kartik-v/yii2-tabs-x": "dev-master",
    "kartik-v/yii2-slider": "dev-master",
    "kartik-v/yii2-export": "*",
    "codemix/yii2-localeurls": "^1.2",
    "kartik-v/yii2-editable": "^1.7"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "codeception/specify": "*",
    "codeception/verify": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*",
    "yiisoft/yii2-apidoc": "^2.0@dev"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
